{
    "name": "invoate/console-commands",
    "description": "This is my package console-commands",
    "keywords": [
        "invoate",
        "laravel",
        "console-commands"
    ],
    "homepage": "https://github.com/invoate/console-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Lumby",
            "email": "oliver@invoate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Invoate\\ConsoleCommands\\": "src",
            "Invoate\\ConsoleCommands\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invoate\\ConsoleCommands\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Invoate\\ConsoleCommands\\ConsoleCommandsServiceProvider"
            ],
            "aliases": {
                "ConsoleCommands": "Invoate\\ConsoleCommands\\Facades\\ConsoleCommands"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
